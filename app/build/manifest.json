{
  "manifest_version": 3,
  "name": "ImageSecurity",
  "version": "1.0.0",
  "description": "A simple React app as a Chrome extension",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "nativeMessaging"
  ]
}
